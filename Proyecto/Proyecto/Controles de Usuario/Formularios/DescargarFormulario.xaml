<UserControl x:Class="Proyecto.Controles_de_Usuario.Formularios.DescargarFormulario"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Proyecto.Controles_de_Usuario.Formularios"
             mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Margin="15,10,15,10" Text="Url:"/>
                <TextBox Margin="0,10,15,10" Width="220"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Margin="15,10,15,10" Text="Etiqueta:"/>
                <TextBox Margin="0,10,15,10" Width="220"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
